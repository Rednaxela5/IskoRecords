<Page x:Class="IskoRecords.SearchStudentPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="Search Student">
    <Grid Height="434" Width="600">
        <TextBlock Text="Search Student Page" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="24"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="56,80,0,0" VerticalAlignment="Top" Height="20" Width="70"/>
        <TextBox x:Name="Student" HorizontalAlignment="Left" Margin="165,131,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <TextBox x:Name="Student_Copy" HorizontalAlignment="Left" Margin="165,169,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <TextBox x:Name="Student_Copy1" HorizontalAlignment="Left" Margin="165,199,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
    </Grid>
</Page>
